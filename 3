#include <iostream>
using namespace std;
int main()
{
    int x , z;
    z =0;
    
      while(cin>> x)  //program wczytuje ciag liczb
    {
              
              z = z + x;
            
              cout<< z << endl; // wypisywanie sumy ciÄ…gu liczb
              if(cin.eof()) break;
              
    }
system("pause");
return 0;
}
